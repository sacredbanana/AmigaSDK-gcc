msgctxt "MSG_CFGB_PopPosition (313//)"
msgid "Position of cycle gadget popup menus."
msgstr "Posición de las entradas en botones cíclicos."

msgctxt "MSG_CFGB_IconifyGadget (314//)"
msgid "Window border gadget to iconify an application."
msgstr "Añadir a las ventanas MUI un botón para iconificar."

msgctxt "MSG_CFGB_IconifyIcon (315//)"
msgid "Show an icon on Workbench in iconified state."
msgstr "Mostrar un icono en el Workbench al iconificar."

msgctxt "MSG_CFGB_StartIconified (316//)"
msgid "Start the application in iconified state."
msgstr "Iniciar el programa en modo iconificado."

msgctxt "MSG_CFGB_NormalFont (322//)"
msgid ""
"Default font for all GUI elements. Will also be used for all other font "
"settings if they remain empty."
msgstr ""
"Tipo de letra por omisión para los elemento de la GUI. Se usará también si "
"algún otro ajuste de tipo de letra permanece vacío."

msgctxt "MSG_CFGB_ListFont (323//)"
msgid "Font used within listviews."
msgstr "Tipo de letra a usar dentro de las listas."

msgctxt "MSG_CFGB_TinyFont (324//)"
msgid "Font used if application requests a tiny font."
msgstr ""
"Tipo de letra a usar si la aplicación requiere un tipo de letra pequeña."

msgctxt "MSG_CFGB_FixedFont (325//)"
msgid "Font used if application requests a fixed width font."
msgstr ""
"Tipo de letra de anchura fija a usar dentro de las listas si la aplicación "
"así lo requiere."

msgctxt "MSG_CFGB_TitleFont (326//)"
msgid "Font used for title of framed groups."
msgstr "Tipo de letra para el título del grupo."

msgctxt "MSG_CFGB_BigFont (327//)"
msgid "Font used if application requests a big font."
msgstr ""
"Tipo de letra a usar si la aplicación requiere un tipo de letra grande."

msgctxt "MSG_CFGB_PublicScreen (328//)"
msgid "Name of public screen the program should open on."
msgstr "Nombre de la pantalla pública donde abrir el programa."

msgctxt "MSG_CFGB_Hotkey (329//)"
msgid "Hotkey to iconify and uniconify a program."
msgstr "Tecla rápida para (des)iconificar."

msgctxt "MSG_CFGB_SnapshotGadget (330//)"
msgid "Window border gadget to snapshot window position."
msgstr ""
"Añadir a las ventanas MUI un botón para congelar la posición de la ventana."

msgctxt "MSG_CFGB_PrefsGadget (331//)"
msgid "Window border gadget to call a programs MUI settings."
msgstr ""
"Añadir a las ventanas MUI un botón para ejecutar los ajustes MUI del "
"programa."

msgctxt "MSG_CFGB_PopupGadget (332//)"
msgid "Window border gadget to show a popup menu with all functions."
msgstr ""
"Añadir a las ventanas MUI un botón que muestra un menú desplegable con las "
"funciones de los botones y más."

msgctxt "MSG_CFGB_OnExitWindows (333//)"
msgid "What to do with window positions if a program exits."
msgstr ""
"Qué hacer con las posiciones de las ventanas MUI al salir del programa."

msgctxt "MSG_CFGB_IconifyMenu (336//)"
