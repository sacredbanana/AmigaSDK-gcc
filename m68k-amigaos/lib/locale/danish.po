msgctxt "MSG_CFGB_IconifyGadget (314//)"
msgid "Window border gadget to iconify an application."
msgstr "Knap i vinduesrammen til at ikonificere en applikation."

msgctxt "MSG_CFGB_IconifyIcon (315//)"
msgid "Show an icon on Workbench in iconified state."
msgstr "Viser et ikon på Workbench ved ikonificering."

msgctxt "MSG_CFGB_StartIconified (316//)"
msgid "Start the application in iconified state."
msgstr "Starte applikationen i ikonificeret tilstand."

msgctxt "MSG_CFGB_NormalFont (322//)"
msgid ""
"Default font for all GUI elements. Will also be used for all other font "
"settings if they remain empty."
msgstr ""
"Standard-skriftsnit for alle brugerfladeelementer. Benyttes også til alle "
"andre skriftindstillinger som ikke angives specielt."

msgctxt "MSG_CFGB_ListFont (323//)"
msgid "Font used within listviews."
msgstr "Skriftsnit som benyttes i lister."

msgctxt "MSG_CFGB_TinyFont (324//)"
msgid "Font used if application requests a tiny font."
msgstr ""
"Skriftsnit som benyttes hvis applikationen beder om et lille skriftsnit."

msgctxt "MSG_CFGB_FixedFont (325//)"
msgid "Font used if application requests a fixed width font."
msgstr ""
"Skriftsnit som benyttes hvis applikationen beder om et skriftsnit med fast "
"bredde."

msgctxt "MSG_CFGB_TitleFont (326//)"
msgid "Font used for title of framed groups."
msgstr "Skriftsnit som benyttes til indrammede gruppers titel."

msgctxt "MSG_CFGB_BigFont (327//)"
msgid "Font used if application requests a big font."
msgstr ""
"Skriftsnit som benyttes hvis applikationen beder om et stort skriftsnit."

msgctxt "MSG_CFGB_PublicScreen (328//)"
msgid "Name of public screen the program should open on."
msgstr "Navn på den offentlige skærm programmet skal åbnes på."

msgctxt "MSG_CFGB_Hotkey (329//)"
